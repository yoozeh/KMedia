{
  "name": "k-media",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": "^8.9.1",
    "npm": "^5.5.1",
    "yarn": "^1.3.2"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "buildapp": "ng build",
    "buildserver": "tsc -p tsconfig-server.json",
    "postinstall": "yarn buildserver && NODE_ENV=development yarn buildapp --aot --prod",
    "localbuild": "yarn buildserver && yarn buildapp",
    "localstart": "node dist-server/server/index.js",
    "watchserver": "nodemon dist-server/server/index.js",
    "watch": "concurrently --kill-others \"yarn buildapp --watch\" \"yarn buildserver --watch\" \"yarn watchserver\"",
    "clean": "rimraf ./dist-app ./dist-server ./node_modules ./yarn.lock",
    "deploy": "git add . && git commit -a && git push origin master"
  },
  "private": true,
  "dependencies": {
    "@types/node": "^8.0.53",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "socket.io": "^2.0.4",
    "typescript": ">=2.4.2 <2.5"
  },
  "devDependencies": {
    "@angular/animations": "^5.0.2",
    "@angular/cdk": "^5.0.0-rc0",
    "@angular/cli": "^1.5.2",
    "@angular/common": "^5.0.2",
    "@angular/compiler": "^5.0.2",
    "@angular/compiler-cli": "^5.0.2",
    "@angular/core": "^5.0.2",
    "@angular/flex-layout": "^2.0.0-beta.10-4905443",
    "@angular/forms": "^5.0.2",
    "@angular/http": "^5.0.2",
    "@angular/language-service": "^5.0.2",
    "@angular/material": "^5.0.0-rc0",
    "@angular/platform-browser": "^5.0.2",
    "@angular/platform-browser-dynamic": "^5.0.2",
    "@angular/router": "^5.0.2",
    "@types/express": "^4.0.39",
    "@types/jasmine": "^2.8.2",
    "@types/jasminewd2": "^2.0.3",
    "@types/socket.io": "^1.4.31",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.5.0",
    "core-js": "^2.5.1",
    "hammerjs": "^2.0.8",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "markdown-it": "^8.4.0",
    "nodemon": "^1.12.1",
    "protractor": "^5.2.0",
    "rimraf": "^2.6.2",
    "rxjs": "^5.5.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "zone.js": "^0.8.18"
  }
